{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
      "forin": false,
      "interface-name": false,
      "max-classes-per-file": false,
      "max-line-length": false,
      "no-console": false,
      "object-literal-sort-keys": false,
      "typedef": [true, "call-signature", "parameter", "property-declaration"]
    }
}
